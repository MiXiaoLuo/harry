/* pages/my/my.wxss */
button::after{
  border: none;
  border-radius: 0;
}
.login{
  width: 100%;
  height: 308rpx;
  border-bottom: 2rpx #e4e4e4 solid;
  background: #fff;
}
.login image{
  width: 176rpx;
  height: 176rpx;
  border-radius: 88rpx;
  display: block;
  margin: 68rpx 0 0 140rpx;
  float: left;
}
.login text{
  display: block;
  float: left;
  line-height: 308rpx;
  padding-left: 80rpx;
  color: #101010;
  font-size: 36rpx;
}
.my{
  width: 100%;
  height: 80rpx;
  font-size: 28rpx;
  color: #101010;
  line-height: 80rpx;
  border-bottom: 2rpx #e4e4e4 solid;
  padding-left: 20rpx;
}
